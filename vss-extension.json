{
  "manifestVersion": 1,
  "id": "jessehouwing-vsts-tfvc-tasks",
  "name": "TFVC Build Tasks",
  "version": "2.0.0",
  "publisher": "jessehouwing",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Build extensions that enable you to interact with TFVC repositories.",
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "tfvc",
    "build",
    "checkin",
    "delete",
    "add",
    "Source Control",
    "Xpirit"
  ],
  "screenshots": [
    {
      "path": "extension/Images/Screenshots/tfvc-tasks.png"
    },
    {
      "path": "extension/Images/Screenshots/tfvc-checkin.png"
    },
    {
      "path": "extension/Images/Screenshots/tfvc-add.png"
    }
  ],
  "content": {
    "details": {
      "path": "readme.md"
    },
    "license": {
      "path": "license.txt"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/jessehouwing/azure-pipelines-tfvc-tasks/wiki"
    },
    "support": {
      "uri": "https://github.com/jessehouwing/azure-pipelines-tfvc-tasks/issues"
    }
  },
  "badges": [
    {
      "href": "https://paypal.me/jessehouwing/5",
      "uri": "https://raw.githubusercontent.com/jessehouwing/azure-pipelines-tfvc-tasks/master/extension/Images/donate.png",
      "description": "Donate"
    },
    {
      "href": "//gitter.im/jessehouwing/vsts-tfvc-tasks?utm_source=badge&utm_medium=badge&utm_campaign=pr-badge",
      "uri": "//badges.gitter.im/jessehouwing/vsts-tfvc-tasks.svg",
      "description": "Chat"
    },
    {
      "href": "https://dev.azure.com/jessehouwing/azure-devops-extensions/_build/latest?definitionId=49&branchName=master",
      "uri": "https://dev.azure.com/jessehouwing/azure-devops-extensions/_apis/build/status/azure-pipelines-tfvc-tasks/azure-pipelines-tfvc-tasks-release?branchName=master",
      "description": "Build"
    }
  ],

  "repository": {
    "type": "git",
    "uri": "https://github.com/jessehouwing/azure-pipelines-tfvc-tasks"
  },
  "branding": {
    "color": "rgb(36, 43, 50)",
    "theme": "dark"
  },
  "icons": {
    "default": "extension/images/extension-icon.png"
  },

  "files": [
    {
      "path": "tf-vc-add"
    },
    {
      "path": "tf-vc-checkout"
    },
    {
      "path": "tf-vc-checkin"
    },
    {
      "path": "tf-vc-delete"
    },
    {
      "path": "tf-vc-undo"
    },
    {
      "path": "tf-vc-shelveset-update"
    }
  ],
  "contributions": [
    {
      "id": "vsts-tfvc-add",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tf-vc-add"
      }
    },
    {
      "id": "vsts-tfvc-checkin",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tf-vc-checkin"
      }
    },
    {
      "id": "vsts-tfvc-checkout",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tf-vc-checkout"
      }
    },
    {
      "id": "vsts-tfvc-delete",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tf-vc-delete"
      }
    },
    {
      "id": "vsts-tfvc-undo",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tf-vc-undo"
      }
    },
    {
      "id": "vsts-tfvc-updateshelveset",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tf-vc-shelveset-update"
      }
    }
  ]
}
